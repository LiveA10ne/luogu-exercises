		arr[1] = 3;
		arr[2] = 4;
		arr[3] = 2;
		pw.print(f(3, 4));
//		pw.print(ans);
		pw.close();
		
	}
	
	private static int f(int i, int j) {
		if (dp[i][j] > 0) {
			return dp[i][j];
		}else if (j < 1) {
			dp[i][j] = f(i - 1, j);
			return dp[i][j];
		}else {
			dp[i][j] = Math.max(f(i - 1, j), f(i - 1, j - 1) + arr[i] );
			return dp[i][j];
		}
	}
}
