
import java.util.Scanner;


public class Main {


	public static void main(String[] args) {
		// TODO Auto-generated method stub	
		Scanner scanner = new Scanner(System.in);
		String express = scanner.nextLine();
		if (express.charAt(express.length() - 1) != '=') {
			int index = express.indexOf('=');
			express = express.substring(0, index - 1);
		}else if (express.charAt(express.length() - 1) == '=') {
			express = express.substring(0, express.length() - 1);
		}
		String [] numbers = express.trim().split("\\D");
		String [] operators = express.substring(1).trim().split("\\d+");
		

		try {
			int sum = Integer.parseInt(numbers[0]);
			int flag = -1;
			for (int i = 0; i < operators.length; i++) {
				if (operators[i].equals("+")) {
					sum = sum+Integer.parseInt(numbers[i+1]);
				}else if (operators[i].equals("-")) {
					sum = sum-Integer.parseInt(numbers[i+1]);
				}else if (operators[i].equals("*")) {
					sum = sum*Integer.parseInt(numbers[i+1]);
				}else if (operators[i].equals("/")) {
					sum = sum/Integer.parseInt(numbers[i+1]);
				}else {
					flag = 1;
					System.out.println("ERROR");
					break;
				}	
			}
			if (flag== -1) {
				System.out.println(sum);
			}

		} catch (Exception e) {
			// TODO: handle exception
			System.out.println("ERROR");
		}
		
		
		scanner.close();
	}
}


